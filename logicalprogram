package logicalprograms;

public class ArmStrongNumber 
{

	public static void main(String[] args) 
	{
int n=153;
int temp=n;
int sum=0;
int remainder=0;
while(n!=0)
{
	remainder=n%10;
	sum=sum+(remainder*remainder*remainder);
	n=n/10;
}
if(temp==sum)
{
	System.out.println("armstrong number");
}
else
{
	System.out.println("not a armstrong number");
			
	
} 
	
	}

}
